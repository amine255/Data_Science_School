public class Main {

	
	private static byte[] readFromFile(String filename, long position, int size)
			throws IOException {
		RandomAccessFile file = new RandomAccessFile(filename, "r");
		file.seek(position);
		byte[] bytes = new byte[size];
		file.read(bytes);
		file.close();
		
		return bytes;
		

	}
	
